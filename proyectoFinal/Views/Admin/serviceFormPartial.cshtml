@model List<proyectoFinal.Models.User>
<form  id="addServiceId">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="clientID">Description</label>
                <input type="text" id="description" class="form-control" placeholder="Doesn't work" required>
            </div>
            <div class="form-group">
                <label for="companyName">Client</label>
                <select class="custom-select form-control" id="clientIDs" required>
                    <option selected>Choose</option>
                    @foreach (proyectoFinal.Models.Client c in (List<proyectoFinal.Models.Client>)Session["Clients"])
                {
                    <option value="@c.id">@c.companyName</option>
                 }

                </select>
            </div>
            <div class="form-group">
                <label for="phoneNumber">Admin</label>
                <select class="custom-select form-control" id="adminID" required>
                    <option selected>Choose</option>
                    @foreach (proyectoFinal.Models.User u in Model)
                {
                    if (u.typeUser == "Administrator")
                    {
                        <option value="@u.id">@u.username</option>
                }

            }
                </select>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="userIDService">User</label>
                <select class="custom-select form-control" id="userIDs" required>
                    <option selected>Choose</option>
                    @foreach (proyectoFinal.Models.User u in Model)
                {
                    if (u.typeUser == "Normal")
                    {
                        <option value="@u.id">@u.username</option>
                }

            }
                </select>
            </div>
            <div class="form-group">
                <label for="email">Date</label>
                <input type="date" id="date" class="form-control" placeholder="" />
            </div>
            <div class="form-group">
                <label>Type of service</label>
                <select class="custom-select form-control" id="typeService" required>
                    <option selected value="">Choose...</option>
                    <option value="0">Mto Preventivo</option>
                    <option value="1">Mto Correctivo</option>
                    <option value="2">Instalacion y Capacitacion</option>
                </select>
            </div>
            <input type="hidden" value="1" id="status" /> <!--everysingle service starts open-->
        </div>
    </div>
        
    
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-primary" onclick="addService()" id="SAVE">Save</button>
    </div>
</form>
